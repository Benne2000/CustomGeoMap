{
  "id": "geoMapWidget2",
  "name": "Geo Map Widget",
  "description": "Performantes Leaflet-Widget mit SAC-Datenbindung.",
  "vendor": "Benne",
  "version": "1.3.1",
  "newInstancePrefix": "GeoMap",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "geo-map-widget",
      "url": "https://benne2000.github.io/CustomGeoMap/main2.js?v=20240808",
	"integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "plzPropertyName": {
      "type": "string",
      "default": "plz",
      "description": "Property-Name in GeoJSON, das die PLZ enth√§lt"
    }
  },
  "dataBindings": {
    "myDataSource": {
      "feeds": [
        {
          "id": "dimensions",
          "description": "Dimension: PLZ",
          "type": "dimension"
        },
        {
          "id": "measures",
          "description": "Kennzahl: Wert",
          "type": "mainStructureMember"
        }
      ]
    }
  },
  "methods": {
    "openSelectModelDialog": {
      "parameters": [],
      "body": "this.dataBindings.getDataBinding('myDataSource').openSelectModelDialog();"
    },
    "getDimensions": {
      "parameters": [],
      "body": "return this.dataBindings.getDataBinding('myDataSource').getDataSource().getDimensions();",
      "returnType": "DimensionInfo[]"
    },
    "getMeasures": {
      "parameters": [],
      "body": "return this.dataBindings.getDataBinding('myDataSource').getDataSource().getMeasures();",
      "returnType": "MeasureInfo[]"
    }
  },
  "events": {}
}
